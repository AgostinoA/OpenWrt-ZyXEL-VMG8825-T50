menu "en75xx configuration"
	depends on TARGET_en75xx
# Following configurations are porting from Econet SDK2.0 7.3.235.200.
#  1. Add prefix "EN7521SDK_" for all configurations
#  2. Remove unused/unsupported CPU configurations
#  3. Refine syntax to avoid output warning messages when doing "make menuconfig" in OpenWRT build system.

#
# ZyXEL configuration
#
menu "ZyXEL configuration on en75xx"
	depends on TARGET_en75xx
# Following configurations are ZyXEL proprietary.
# Add prefix "EN7521SDK_" for all configurations

config EN7521SDK_ZYXEL
	bool "ZyXEL customized"
	default y

config EN7521SDK_ZYXEL_DEFCFG_FILENAME
	depends on EN7521SDK_ZYXEL_SUPPORT_DEFCFG
	string "The file name of defcfg image in $(SUBTARGET)/defconfig/"
	default "romfile.cfg"

config EN7521SDK_ZYXEL_MRD_FILENAME
	string "The file stored MRD informations in $(SUBTARGET)/"
	default "mi.conf"

config EN7521SDK_ZYXEL_LED_FILENAME
	string "The file stored LED definitions in $(SUBTARGET)/"
	default "led.conf"

config EN7521SDK_ZYXEL_SUPPORT_DEFCFG
	depends on EN7521SDK_ZYXEL
	bool "Support the defcfg partition, inside tclinux.bin"
	default y

config EN7521SDK_ZYXEL_SUPPORT_ROMD
	depends on EN7521SDK_ZYXEL
	bool "Support the rom-d partition"
	default y

config EN7521SDK_ZYXEL_SUPPORT_ROMFILE2
	depends on EN7521SDK_ZYXEL && !EN7521SDK_ZYXEL_SUPPORT_DATA
	bool "Support the second romfile partition"
	default n

config EN7521SDK_ZYXEL_SUPPORT_DATA
	depends on EN7521SDK_ZYXEL
	bool "Support the data partition"
	default n

config EN7521SDK_ZYXEL_SUPPORT_WWANPKG
	depends on EN7521SDK_ZYXEL
	bool "Support the wwanpkg partition"
	default n

config EN7521SDK_ZYXEL_SUPPORT_DUAL_IMAGE
	depends on EN7521SDK_ZYXEL
	bool "Support the daul image with ZyXEL style"
	default n

config EN7521SDK_ZYXEL_SUPPORT_USB_PROTECTION_CIRCUIT
	depends on EN7521SDK_ZYXEL
	bool "Add additional power switch fo USB to prevent over current"
	default n

config EN7521SDK_ZYXEL_USB_OC_ENABLE_GPIO
	int "Which GPIO for enable pin of power switch connect on SoC"
	default 2
	depends on EN7521SDK_ZYXEL_SUPPORT_USB_PROTECTION_CIRCUIT

config EN7521SDK_ZYXEL_USB_OC_FLAG_SUPPORT
        depends on EN7521SDK_ZYXEL_SUPPORT_USB_PROTECTION_CIRCUIT
        bool "Support GPIO flag pin of power switch connect on SoC"
        default n

config EN7521SDK_ZYXEL_USB_OC_FLAG_GPIO
	int "Which GPIO for flag pin of power switch connect on SoC"
	default 3
	depends on EN7521SDK_ZYXEL_SUPPORT_USB_PROTECTION_CIRCUIT && EN7521SDK_ZYXEL_USB_OC_FLAG_SUPPORT

config EN7521SDK_ZYXEL_CUSTOMIZE_LED_BEHAVIOR
	depends on EN7521SDK_ZYXEL
	bool "ZyXEL add properity LED interface and customize the LED hahavior"
	default y

config EN7521SDK_ZYXEL_INVERSE_GPON_LOS_LED
	depends on EN7521SDK_ZYXEL
	bool "Let GPON los LED become active LOW, reference design is active high."
	default n

config EN7521SDK_ZYXEL_DISABLE_SECOND_PCI
	depends on EN7521SDK_ZYXEL
	bool "Disable the second PCI to release GPIO_14 for general perpose(LED)"
	default n

config EN7521SDK_ZYXEL_PCIE_RESET0_PIN_AS_GPIO_30
	depends on EN7521SDK_ZYXEL
	bool "Let PCIE RESET0 pin output GPIO 30 function"
	default n
config EN7521SDK_ZYXEL_PCIE_RESET1_PIN_AS_GPIO_31
	depends on EN7521SDK_ZYXEL
	bool "Let PCIE RESET1 pin output GPIO 31 function"
	default n

config EN7521SDK_ZYXEL_GE_LED_PIN_AS_GPIO_3
	depends on EN7521SDK_ZYXEL
	bool "Let GE LED pin output GPIO 3 function on EN75xx platform"
	default n

config EN7521SDK_ZYXEL_INTERNAL_SWITCH_LED_PIN_AS_GPIO
	depends on EN7521SDK_ZYXEL
	bool "Let Internal switch LED pin output GPIO(3, 7, 8, 9, 10) function on EN75xx platform"
	default n

config EN7521SDK_ZYXEL_FW_UPGRADE_POWER_LED_BLINK
	depends on EN7521SDK_ZYXEL_CUSTOMIZE_LED_BEHAVIOR
	bool "Support FW Upgrade Power LED blinking control"
	default y

config EN7521SDK_ZYXEL_WLAN_WPS_USE_SAME_LED
	depends on EN7521SDK_ZYXEL_CUSTOMIZE_LED_BEHAVIOR
	bool "Support WLAN and WPS working on the same LED!"
	default n

config EN7521SDK_ZYXEL_UPS_FEATURE
        depends on EN7521SDK_ZYXEL
        bool "Support UPS feature"
        default y

config EN7521SDK_ZYXEL_LAN_WAN_WLAN_SHARE_THE_SAME_MAC
	bool "LAN, WAN1, and WLAN1 use the same MAC"
	depends on EN75XXSDK_ZYXEL
	default n
	help
		For ECONET platform, MAC quantity should be 8 or 16.
		For single band projects, GDM MASK should be 0xf8, MAC quantity will be 8,
		and LAN, WAN1, and WLAN1 use the same MAC.
		For dual  band projects, GDM MASK should be 0xf0, MAC quantity will be 16,
		and WLAN1=LAN+1, WLAN2=LAN+2, WAN1=LAN+3, ...

endmenu

