+use strict;
+
+my $ipv4_rex = qr/(?:\d{1,3}\.){3}\d{1,3}/imosx;
+
+sub print_usage($) {
+
+    my $file = shift;
+
+    print($file "Usage: interface.pl IFNAME\n");
+
+}
