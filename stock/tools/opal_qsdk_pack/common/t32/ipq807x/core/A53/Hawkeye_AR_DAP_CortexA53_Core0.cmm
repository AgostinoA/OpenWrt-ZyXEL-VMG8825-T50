local &arg1
entry &arg1

area.view
print "value 1: &arg1"

SYStem.Reset
SYStem.CPU CORTEXA53
SYSTEM.JtagClock Ctck 20.0MHz
SYStem.CONFIG SLAVE OFF

&cur_dir=os.ppd()    	; directory where this script is running from

; select CPU and options

;;ntimescc  ; time out ignored only for debugging 
SYStem.Option EnReset OFF
TrOnchip.Set reset  OFF

SYStem.CONFIG	DAPIRPRE	0.
SYStem.CONFIG	DAPDRPRE	0.
SYStem.CONFIG	DAPIRPOST	16.
SYStem.CONFIG	DAPDRPOST	2.

SYStem.CONFIG.COREDEBUG.BASE	0x00190000
SYStem.CONFIG.ETM.BASE			0x0019C000
SYStem.CONFIG.CTI.BASE			0x00198000

SYStem.config AXIACCESSPORT 0
SYStem.config MEMORYACCESSPORT 0
SYStem.config DEBUGACCESSPORT 1
SYStem.Option EnReset ON
ETM.OFF
sys.attach


autostore , history
ENDDO

